<div class="button22 clickable" (click)="GoBack()"><div class="fa fa-arrow-left"></div> Back &nbsp;</div>
<p *ngIf="error" class="cardErrorMessage">{{error}}</p>

<div class="ui cards">
  <div class="card" *ngIf="student">
    <div class="content">
      <div class="cardHeader">
          <legend>{{student.firstname}} {{student.lastname}}</legend>
      </div>
      <div class="meta">
        <div class="cardLeft">Age:</div> <div class="cardRight">{{student.dob}}</div><br>
        <div class="cardLeft">Instrument:</div> <div class="cardRight">{{student.instrument}}</div><br>
        <div class="cardLeft">Grade:</div> <div class="cardRight">{{student.grade}}</div><br>
        <div class="cardLeft">Start Time:</div> <div class="cardRight">{{student.starttime}}</div><br>
        <div class="cardLeft">End Time:</div> <div class="cardRight">{{student.endtime}}</div><br>
        <div class="cardLeft">Term:</div> <div class="cardRight">{{student.term}}</div><br>
        <div class="cardLeft">Year:</div> <div class="cardRight">{{student.year}}</div><br>
        <div class="cardLeft">Fee:</div> <div class="cardRight">${{student.fee}}</div><br>
        <div class="cardLeft">Notes:</div> <div class="cardRight">{{student.notes}}</div><br>
      </div>
    </div>
    <div class="extra content">
      <div class="ui two buttons">
         <button class="button22" (click)="AcceptStudent(student.requestid)">Approve</button>
         <button class="button21" (click)="RejectStudent(student.requestid)">Decline</button>
      </div>
    </div>
  </div>
</div>